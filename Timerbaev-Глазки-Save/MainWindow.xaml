<Window x:Class="Timerbaev_Глазки_Save.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Timerbaev_Глазки_Save"
        mc:Ignorable="d"
        Title="Timerbaev-Глазки" FontFamily=" Century Gothic" Icon="Resources/Глазки-save.png" Height="450" Width="800" MinHeight="200" MinWidth="700">
    
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="75"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="30"/>
        </Grid.RowDefinitions>
        <Image Source="Resources/Глазки-save.png" HorizontalAlignment="Left"></Image>
        <TextBlock Text="Агенты" FontSize="30" HorizontalAlignment="Center" VerticalAlignment="Center"></TextBlock>
        <Frame Grid.Row="1" Name="MainFrame" ContentRendered="MainFrame_ContentRendered" NavigationUIVisibility="Hidden"></Frame>
        <Button Content="Назад" Name="BtnBack" Width="175" Height="30" HorizontalAlignment="Right" Margin="5"
                Click="BtnBack_Click"></Button>
    </Grid>
</Window>
